CREATE TABLE Projects ( 
    id INT NOT NULL AUTO_INCREMENT,
    mainPictureUrl TEXT NOT NULL,
    title VARCHAR (200) NOT NULL,
    description TEXT NOT NULL,
    PRIMARY KEY (id));

