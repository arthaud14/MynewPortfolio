CREATE TABLE Projects ( 
    id INT NOT NULL AUTO_INCREMENT,
    mainPictureUrl TEXT NOT NULL,
    project VARCHAR (200) NOT NULL,
    title VARCHAR (200) NOT NULL,
    description TEXT NOT NULL,
    technology TEXT NOT NULL,
    team TEXT NOT NULL,
    url TEXT NOT NULL,
    PRIMARY KEY (id));

